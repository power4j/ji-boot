<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.power4j.flygon.admin.modules.security.dao.ApiTokenMapper">
  <select id="selectForUpdate" resultType="com.power4j.flygon.admin.modules.security.entity.ApiTokenEntity">
    select * from `t_api_token` t where t.username = #{username} for update;
  </select>
</mapper>
